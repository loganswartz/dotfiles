{
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte",
    "python",
    "json"
  ],
  "tsserver.formatOnType": false,
  "coc.preferences.formatOnType": false,
  "snippets.ultisnips.directories": ["$HOME/.config/nvim/vim-plug"],
  "snippets.extends": {
    "typescriptreact": ["javascript"]
  },
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "html",
    "HTML (EEx)",
    "HTML (Eex)",
    "html.twig",
    "jade",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue"
  ],
  "coc.preferences.floatActions": false,
  "python.pythonPath": "python3",
  "python.formatting.provider": "black",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.autoImportCompletions": true
  /*
  "languageserver": {
    "pylance": {
      "enable": false,
      "filetypes": ["python"],
      "module": "~/.config/coc/extensions/pylance/extension/dist/server.bundle.js",
      "initializationOptions": {},
      "settings": {
        "python.analysis.diagnosticSeverityOverrides": {
          "reportUndefinedVariable": "error"
        }
      }
    }
  }
  */
}
